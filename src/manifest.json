{
  "name": "new the100 calendar 2",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://www.the100.io/*"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
